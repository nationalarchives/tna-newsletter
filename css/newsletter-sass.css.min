.newsletter-template{background:#ddd;width:100%;box-sizing:border-box;overflow:hidden}.newsletter-template .news-icon{background:#008484;vertical-align:middle;text-align:center;width:157px;height:127px;display:inline-block;padding:38px 0 0;float:left;overflow:hidden}.newsletter-template .news-icon img{width:60px;height:48px}@media (max-width:1155px){.newsletter-template{border-left:15px solid #008484}.newsletter-template .news-icon{display:none}}.newsletter-template .news-content{width:55%;display:inline-block;float:left}@media (max-width:970px){.newsletter-template .news-content{width:60%}}@media (max-width:767px){.newsletter-template .news-content{display:block;width:100%}}.newsletter-template .news-form{width:30%;display:inline-block;padding:45px 0 0;float:left}.newsletter-template .news-form form input[type=email]{width:65%;height:37px}@media (max-width:370px){.newsletter-template .news-form form input[type=email]{width:100%;margin-bottom:10px}}.newsletter-template .news-form form input[type=submit]{height:37px;border:none;background:#008484;color:#fff;font-size:16px;padding:0 10px}@media (max-width:1155px){.newsletter-template .news-form{padding:18px 0 0}}@media (max-width:970px){.newsletter-template .news-form{width:40%}}@media (min-width:768px) and (max-width:969px){.newsletter-template .news-form{padding:18px 0 16px 7px}}@media (max-width:767px){.newsletter-template .news-form{display:block;padding:0 0 10px 7px;width:80%}}@media (max-width:370px){.newsletter-template .news-form{width:100%;padding-right:10px}}.newsletter-template h2{margin:0;padding:36px 0 0 20px;font-weight:300;font-size:24px;font-family:Bitter,serif}@media (max-width:1155px){.newsletter-template h2{padding:10px 15px 10px 7px}}.newsletter-template p{padding:10px 0 0 20px}@media (max-width:1155px){.newsletter-template p{padding:0 0 0 7px}}.newsletter-template label{display:none}
